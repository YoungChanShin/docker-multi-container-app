{
    "AWSEBDockerrunversion": 2,
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "chan8149/docker-frontend",
            "hostname": "frontend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "backend",
            "image": "chan8149/docker-backend",
            "hostname": "backend",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "chan8149/docker-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "link": [
                "frontend",
                "backend"
            ]
        }
    ]
}